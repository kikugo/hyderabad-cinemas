{
  "name": "hyderabad-cinemas",
  "version": "1.0.0",
  "description": "Interactive guide to 98 movie theaters across Hyderabad - from iconic single screens to premium multiplexes",
  "private": true,
  "type": "module",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "format": "prettier --write .",
    "scrape-images": "node scripts/scrape-theater-images.js",
    "scrape-images:debug": "node scripts/scrape-theater-images.js --headless=false",
    "fetch-images": "node scripts/fetch-theater-images.js",
    "fetch-images:dry": "node scripts/fetch-theater-images.js --dry-run"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "prettier": "^3.3.3",
    "postcss": "^8.4.32",
    "puppeteer": "^24.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
